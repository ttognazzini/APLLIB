Ctl-Opt Option(*SrcStmt) DftActGrp(*No) ActGrp(*New) Main(Main);

// Build APLLIB - Initial File Seeds

// This member creates all files for the dta system and seeds the with some required data
// the intent is to allow the dictionary and file system to be moved to a new library and
// started from scratch.

// For testing the system has been move the library APLLIB. This member hard references
// that library, change it to where you want this to run.

// create the dictionary, fields and segments files

// default SQL options, rarely do any of these change
Exec SQL Set Option Commit = *none, UsrPrf = *owner, datfmt = *ISO, DynUsrPrf = *owner, CloSQLCsr = *endmod,
                    SrtSeq = *langidshr;

Dcl-Proc Main;

  // Build DCTMST
  Exec SQL
    Create or Replace Table APLLIB/DCTMST (
        active_row for Column AcvRow Char(1) not null with default '1',
        dictionary_master_id for Column dctMstIdn bigInt
            not null generated always as identity (start with 1 increment by 1),
        dictionary_name for Column dctNme char(10) not null with default,
        description for Column des char(40) not null with default,
        note_ for Column nte char(60) not null with default,
        creation_time_stamp for Column crtDtm timestamp not null with default current timestamp,
        creation_user for Column crtUsr char(18) not null with default user,
        creation_job for Column crtJob char(28) not null with default,
        creation_program for Column crtPgm char(10) not null with default,
        maintenance_time_stamp for Column mntDtm timeStamp not null with default current timestamp,
        maintenance_user for Column mntUsr char(18) not null with default user,
        maintenance_job for Column mntJob char(28) not null with default,
        maintenance_program for Column MntPgm char(10) not null with default
        )
      RcdFmt DCTmstR;

  Exec SQL Alter Table APLLIB.DCTMST add Constraint APLLIB.DCTMST Primary Key( DctNme );

  Exec SQL Label on Table APLLIB/DCTMST is 'Dictionary Master File';

  Exec SQL
    Label on Column APLLIB.DCTMST
    (ACTIVE_ROW is 'Active              Row' ,
      DICTIonARY_MASTER_ID is 'Dictionary          Master              Id' ,
      DICTIonARY_NAME is 'Dictionary          Name' ,
      DESCRIPTIon is 'Description' ,
      NOTE_ is 'Note' ,
      CREATIon_TIME_STAMP is 'Creation            Date' ,
      CREATIon_USER is 'Creation            User' ,
      CREATIon_JOB is 'Creation            Job' ,
      CREATIon_PROGRAM is 'Creation            Program' ,
      MAINTENANCE_TIME_STAMP is 'Maintenance         Date' ,
      MAINTENANCE_USER is 'Maintenance         User' ,
      MAINTENANCE_JOB is 'Maintenance         Job' ,
      MAINTENANCE_PROGRAM is 'Maintenance         Program' ) ;

  Exec SQL
    Label on Column APLLIB.DCTMST
    (ACTIVE_ROW TEXT is 'Active Row' ,
      DICTIonARY_MASTER_ID TEXT is 'Dictionary Master ID' ,
      DICTIonARY_NAME TEXT is 'Dictionary Name' ,
      DESCRIPTIon TEXT is 'Description' ,
      NOTE_ TEXT is 'Note' ,
      CREATIon_TIME_STAMP TEXT is 'Creation Date' ,
      CREATIon_USER TEXT is 'Creation User' ,
      CREATIon_JOB TEXT is 'Creation Job' ,
      CREATIon_PROGRAM TEXT is 'Creation Program' ,
      MAINTENANCE_TIME_STAMP TEXT is 'Maintenance Date' ,
      MAINTENANCE_USER TEXT is 'Maintenance User' ,
      MAINTENANCE_JOB TEXT is 'Maintenance Job' ,
      MAINTENANCE_PROGRAM TEXT is 'Maintenance Program' ) ;

