        force_length_3 for Column FRCLEN3 INTEGER not null default 0 ,
        force_length_4 for Column FRCLEN4 INTEGER not null default 0 ,
        force_length_5 for Column FRCLEN5 INTEGER not null default 0 ,
        DEFAULT_VALUE for Column DFTVAL CHAR(60) not null default '' ,
        SYSTEM_TYPE for Column SYSTYP CHAR(10) not null default '' ,
        SYSTEM_LENGTH for Column SYSLEN DECIMAL(20, 0) not null default 0 ,
        creation_time_stamp for Column crtDtm timestamp not null with default current timestamp,
