        field_ for Column FLDTYP CHAR(14) not null default '' ,
        description for Column DES CHAR(40) not null default '' ,
        is_numeric for Column NMR CHAR(2) not null default '' ,
        allow_decimal_positions for Column ALWDEC CHAR(1) not null default '' ,
        allow_length for Column ALWLEN CHAR(1) not null default '' ,
        require_length for Column REQLEN CHAR(1) not null default '' ,
        allow_allocation for Column ALWALC CHAR(1) not null default '' ,
        require_allocation for Column REQALC CHAR(1) not null default '' ,
        maximum_length for Column MAXLEN BIGINT not null default 0 ,
        largest_value for Column LRGVAL DECIMAL(63, 0) not null default 0 ,
