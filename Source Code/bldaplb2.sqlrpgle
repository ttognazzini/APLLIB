      CRTPGM is 'Creation            Program' ,
      MNTDTM is 'Maintenance         Date' ,
      MNTUSR is 'Maintenance         User' ,
      MNTJOB is 'Maintenance         Job' ,
      MNTPGM is 'Maintenance         Program' ) ;

  Exec SQL
