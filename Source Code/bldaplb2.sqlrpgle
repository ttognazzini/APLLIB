    (ACTIVE_ROW TEXT is 'Active Row' ,
      DICTIonARY_FIELD_ID TEXT is 'Dictionary Field ID' ,
      DICTIonARY_NAME TEXT is 'Dictionary Name' ,
      FIELD_NAME TEXT is 'Field Name' ,
